<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eliotshekhtman.clickerquest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.eliotshekhtman.clickerquest.MainActivity"
            android:label="Clicker Quest">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.eliotshekhtman.clickerquest.Quest"
            android:label="Quest">
        </activity>
        <activity android:name="com.eliotshekhtman.clickerquest.Market"
            android:label="Market">
        </activity>
        <activity android:name="com.eliotshekhtman.clickerquest.Enhancement_tent"
            android:label="Enhancement tent">
        </activity>
        <activity android:name="com.eliotshekhtman.clickerquest.Blacksmith_tent"
            android:label="Blacksmith tent">
        </activity>
        <activity android:name="com.eliotshekhtman.clickerquest.App"></activity>
    </application>
</manifest>